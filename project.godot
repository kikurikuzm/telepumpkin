2d_physics/layer_2="playerCollision"
2d_physics/layer_3="pumpkinInteraction"
2d_physics/layer_5="playerInteraction"
2d_physics/layer_6="peaterLayer"

[physics]

2d/run_on_separate_thread=true
common/max_physics_steps_per_frame=32
2d/default_gravity=200.0
2d/solver/solver_iterations=64
common/enable_pause_aware_picking=true

[rendering]

textures/canvas_textures/default_texture_filter=0
