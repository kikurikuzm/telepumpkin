; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Telepumpkin 2"
run/main_scene="res://Instances/mainMenu.tscn"
config/features=PackedStringArray("4.2")
boot_splash/bg_color=Color(0.423529, 0.337255, 0.443137, 1)
boot_splash/image="res://Sprites/Misc/icon.png"
boot_splash/fullsize=false
config/icon="res://Sprites/Misc/icon.png"
boot_splash/minimum_display_time=25

[audio]

buses/default_bus_layout="res://Tile Sets/default_bus_layout.tres"
default_bus_layout="res://Audio/audio.tres"

[autoload]

gvars="*res://Scripts/gvars.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/resizable=false
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=3
window/use_physical_light_units=false

[editor]

naming/scene_name_casing=1

[editor_plugins]

enabled=PackedStringArray()

[file_customization]

folder_colors={
"res://Audio/": "yellow",
"res://Levels/": "teal",
"res://Scripts/": "red",
"res://Sprites/": "pink"
}

